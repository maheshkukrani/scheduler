    "copy-storybook-assets": "mkdir -p storybook-static/public/ico && cp ./node_modules/@idexx/spot/dist/icons/spot_icons.svg ./storybook-static/public/ico/spot_icons.svg",
    "dep-graph": "nx dep-graph",
    "e2e": "nx run-many --target=e2e --all",
